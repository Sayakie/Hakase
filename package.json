  "license": "MIT",
  "engines": {
    "node": ">=16.9",
    "pnpm": "^6"
  },
  "devEngines": {
    "node": ">=16.13",
    "pnpm": "^6.32.3"
  },
  "packageManager": "pnpm@^6.32.3",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "postinstall": "node scripts/check-dev-engines.mjs package.json && node scripts/dev-patch.mjs && pnpm run util:update-git-hooks",
    "start": "NODE_ENV=production node --enable-source-maps dist/index.js",
    "dev": "NODE_ENV=development node dist/index.js",
    "docs": "typedoc",
    "test": "jest",
    "build": "rimraf dist && swc src --out-dir dist",
    "build:watch": "tsc --watch --skipLibCheck",
    "lint": "eslint --ext .ts,.mts,.cts src/",
    "lint:fix": "pnpm run lint -- --fix",
    "util:update-git-hooks": "pnpx simple-git-hooks"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "discord-api-types": "^0.31.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "i18next": "^21.6.15",
    "i18next-fs-backend": "^1.1.4",
    "immutable": "^4.0.0",
    "korean-regexp": "^1.0.9",
    "merge-options": "^3.0.4",
    "picocolors": "^1.0.0",
    "renamer": "^4.0.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.6",
    "walk-sync": "^3.0.0",
    "zx": "^5.3.0"
  },
